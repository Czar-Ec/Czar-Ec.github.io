  "scripts": {
    "cypress-open": "cypress open",
    "cypress-run": "cypress run",
    "ng": "ng",
    "start": "ng serve --open --host czarec --port 80",
    "start:ci": "ng serve --disableHostCheck=false --host 0.0.0.0 --port 8080",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test --watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:e2e": "start-server-and-test start:ci \"http-get://localhost:8080/\" cypress-open",
    "test:ci": "ng test --watch=false --codeCoverage=true",
    "test:ci:e2e": "yarn cypress cache clear && yarn cypress install && yarn setup:ci:e2e",
    "setup:ci:e2e": "start-server-and-test start:ci \"http-get://localhost:8080/\" cypress-run"
  },
