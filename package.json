  "scripts": {
    "cypress-open": "cypress open",
    "cypress-run": "cypress run",
    "ng": "ng",
    "start": "ng serve --open --host 0.0.0.0 --port 8080",
    "start:ci": "ng serve --host 0.0.0.0 --port 8080",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "test": "ng test --watch",
    "test:ci": "ng test --watch=false --karma-config \"./src/karma.conf-ci.js\" --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "cy:open": "start-server-and-test start:ci \"http://localhost:8080/assets\" cypress-open",
    "cy:run": "start-server-and-test start:ci \"http://localhost:8080/assets\" ''",
    "cy:ci:e2e": "yarn cypress cache clear && yarn cypress install && yarn setup:ci:e2e",
    "setup:ci:e2e": "start-server-and-test start:ci \"http://localhost:8080/assets\" && cypress-run"
  },
  "repository": {
    "type": "git",
